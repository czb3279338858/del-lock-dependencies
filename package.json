  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/del-lock-dependencies.umd.cjs",
  "module": "./dist/del-lock-dependencies.js",
  "types": "./dist/index.d.ts",
  "exports": {
